{
  "name": "getonbrd-scrapper-jobs",
  "version": "0.2.0",
  "description": "CLI y librería para scrappear empleos del listado de programación en Get on Board.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "scrape": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "pnpm run scrape"
  },
  "keywords": [
    "scraper",
    "getonbrd",
    "jobs",
    "nodejs",
    "cli",
    "pnpm"
  ],
  "author": "GetOnBoard Scraper",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.28.1",
  "dependencies": {
    "@supabase/supabase-js": "^2.91.0",
    "cheerio": "1.1.2",
    "dotenv": "^17.2.3",
    "got": "14.6.6",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
